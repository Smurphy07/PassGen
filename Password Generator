import random
import io


a = ["fast", "slow", "tired", "exhausted", "relieved", "ambiguous", "dangerous", "friendly", "attractive"]
n = ["desk", "pen", "song", "remote", "light", "mouse", "speaker", "wall", "brick", "note", "table"]
p = "0123456789"
passlen = 3

r =''.join(random.choice(a))+''.join(random.choice(n))+ "".join(random.sample(p,passlen))

print("Your password:",(r))
